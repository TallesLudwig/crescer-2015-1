package atorFilme;

import java.util.HashMap;

public class Filme {
	
	private String NomeFilme;
	private HashMap<String, Ator> exercito = new HashMap<>();
	
	   public Filme(String Nome)
	    {
		   this.NomeFilme= Nome;
	        
	    }

	   public HashMap<String, Ator> getExercito() {
		    return this.exercito;
		}

		public void alistar(Ator ator){
		    
		        exercito.put(ator.getNome(), ator);
		    
		}
	   
	   
	   
	public String getNome() {
		return NomeFilme;
	}

}





